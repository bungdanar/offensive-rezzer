          },
          "start_date": {
            "type": "string",
            "format": "date-time"
          },
          "end_date": {
            "type": "string",
            "format": "date-time",
            "description": "Must be greater than or equal to start_date"
          }
        }
      },
      "UserCreatePayload": {
        "type": "object",
        "required": [
          "first_name",
          "last_name",
          "email",
          "phone_code",
          "phone_number"
        ],
        "properties": {
          "first_name": {
            "type": "string",
