            "schema": {
              "type": "integer",
              "nullable": false,
              "minimum": 0,
              "maximum": 9999
            }
          },
          {
            "name": "description",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "nullable": false,
