            "required": false,
            "schema": {
              "type": "number",
              "nullable": false,
              "minimum": 0,
              "maximum": 1000
            }
          },
          {
            "name": "created_at",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time",
              "nullable": false
