            "maxLength": 255
          },
          "description": {
            "type": "string",
            "minLength": 3,
            "maxLength": 1000
          }
        }
      },
      "CouponCreatePayload": {
        "type": "object",
        "required": [
          "code",
          "description",
          "discount_value",
          "discount_type",
          "max_usage",
          "start_date",
          "end_date"
        ],
        "properties": {
          "code": {
            "type": "string",
            "minLength": 3,
            "maxLength": 255
          },
          "description": {
