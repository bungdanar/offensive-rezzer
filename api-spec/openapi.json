          },
          "last_name": {
            "type": "string",
            "minLength": 3,
            "maxLength": 255
          },
          "email": {
            "type": "string",
            "pattern": "^[a-z0-9._+-]{1,20}@[a-z0-9]{3,15}([.][a-z]{2,4}){1,3}$",
            "maxLength": 255
          },
          "phone_code": {
            "type": "string",
            "pattern": "^[0-9]{1,3}$",
            "minLength": 1,
            "maxLength": 3
          },
          "phone_number": {
            "type": "string",
